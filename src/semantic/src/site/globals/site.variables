/*******************************
     User Global Variables
*******************************/

@primaryColor: #9be795;
@primaryColorHover: darken(@primaryColor, 10);
@primaryColorFocus: darken(@primaryColor, 10);
@primaryColorDown: darken(@primaryColor, 15);

@secondaryColor: #d4d4d4;
@secondaryColorHover: #c4c4c4;
@secondaryColorFocus: #c4c4c4;
@secondaryColorDown: #b4b4b4;

@linkColor: @primaryColor;
@linkHoverColor: @primaryColorHover;
